    "class-validator": "^0.14.3",
    "dotenv": "^17.2.3",
    "helmet": "^8.1.0",
    "loglayer": "^8.4.0",
    "next": "16.1.3",
    "octokit": "^5.0.5",
    "pg": "^8.17.2",
    "react": "19.2.3",
    "react-dom": "19.2.3",
