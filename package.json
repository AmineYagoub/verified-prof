{
  "name": "verified-prof",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build:api": "nx run worker:build",
    "dev:api": "nx run worker:serve:development",
    "dev:web": "rimraf apps/web/.next && nx run web:dev --turbopack",
    "build:web": "rimraf apps/web/.next && nx run web:build",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv ./cloudflare-env.d.ts",
    "db:mig": "npx wrangler d1 migrations create verified-prof",
    "db:apply": "npx wrangler d1 migrations apply verified-prof --remote",
    "db:gen": "prisma generate"
  },
  "dependencies": {
