    "@nestjs/core": "^11.1.12",
    "@nestjs/platform-express": "^11.1.12",
    "@prisma/adapter-pg": "^7.2.0",
    "@prisma/client": "^7.2.0",
    "axios": "^1.6.0",
    "better-auth": "^1.4.16",
